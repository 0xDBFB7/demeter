set term {$C->default("gnuplot","terminal")} font '{$C->default("gnuplot","font")},{$C->default("gnuplot","fontsize")}' enhanced
set encoding iso_8859_15

## line styles
set style line 1  linecolor rgb "{$C->default("gnuplot", "c0")}"
set style line 2  linecolor rgb "{$C->default("gnuplot", "c1")}"
set style line 3  linecolor rgb "{$C->default("gnuplot", "c2")}"
set style line 4  linecolor rgb "{$C->default("gnuplot", "c3")}"
set style line 5  linecolor rgb "{$C->default("gnuplot", "c4")}"
set style line 6  linecolor rgb "{$C->default("gnuplot", "c5")}"
set style line 7  linecolor rgb "{$C->default("gnuplot", "c6")}"
set style line 8  linecolor rgb "{$C->default("gnuplot", "c7")}"
set style line 9  linecolor rgb "{$C->default("gnuplot", "c8")}"
set style line 10 linecolor rgb "{$C->default("gnuplot", "c9")}"

## marker styles
set style line 12 linecolor rgb "{$C->default("gnuplot", "markercolor")}"

set style line 20 linecolor rgb "{$C->default("gnuplot", "gridcolor")}"
set grid ls 20
